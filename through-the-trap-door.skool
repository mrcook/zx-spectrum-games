 59746 LD (34273),A                  ; ...
 59749 LD C,(IX+0)                   ; Load outer repeat count into C
 59752 LD B,(IX+1)                   ; Load inner repeat count into B
 59755 LD E,(IX+2)                   ; Load delay constant E into E
